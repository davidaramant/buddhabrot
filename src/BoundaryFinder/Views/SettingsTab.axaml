<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:BoundaryFinder.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BoundaryFinder.Views.SettingsTab"
             x:CompileBindings="True"
             x:DataType="vm:SettingsViewModel">
    <StackPanel Orientation="Vertical">
        <TextBox IsReadOnly="True" Text="{Binding DataSetPath, Mode=OneWay}" />
        <Button Content="Change File Path" Command="{Binding UpdateFilePathCommand}" />
    </StackPanel>
</UserControl>